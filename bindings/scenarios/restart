errors_on()
echo_on()
alice_send("Hi Bob!")
bob_receive()
bob_send("Hi Alice!")
alice_receive()
restart()
alice_send("Bob, are you still there?")
bob_receive()
bob_send("Yes, I'm here!")
alice_receive()
alice_send("2")
bob_receive()
bob_send("3")
alice_receive()
restart()
alice_send("4")
bob_receive()
bob_send("5")
alice_receive()
alice_send("6")
bob_receive()
